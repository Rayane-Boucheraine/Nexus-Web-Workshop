generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         String   @id @default(cuid())
  email      String   @unique
  firstName  String
  familyName String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}